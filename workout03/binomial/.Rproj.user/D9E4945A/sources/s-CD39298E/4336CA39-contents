\name{bin_probability}
\alias{bin_probability}
\title{Binomial Probability}
\usage{
bin_probability(success, trials, prob)
}
\arguments{
\item{success}{vector of success values}

\item{trials}{number of trials}

\item{prob}{probability of a success}
}
\value{
vector of probabilities of k successes for each success value
}
\description{
Calculates probability of k successes in n trials with probability of success p
}
\examples{
bin_probability(1, 4, 0.6)

bin_probability(2:4, 6, 0.8)

bin_probability(success = 2, trials = 5, prob = 0.43dev)
}
