\name{bin_variance}
\alias{bin_variance}
\title{Variance}
\usage{
bin_variance(trials, prob)
}
\arguments{
\item{trials}{number of trials}

\item{prob}{probability of a success}
}
\value{
the computed variance
}
\description{
Calculates the variance of a binomial random variable
}
\examples{
bin_variance(3, 0.7)

bin_variance(trials = 6, prob = 0.4)

}
